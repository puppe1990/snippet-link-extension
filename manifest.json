{
  "manifest_version": 3,
  "name": "Link Manager",
  "version": "1.0",
  "description": "Gerencie seus links favoritos com facilidade",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Link Manager"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
